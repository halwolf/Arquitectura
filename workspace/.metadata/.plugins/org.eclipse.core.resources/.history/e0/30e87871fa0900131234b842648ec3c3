package ejejrcicio1;

public class condicionales {

	public static void main(String[] args) {
	int A=7, B=12, C=22;
	System.out.println("A/2==0,B/2==0,C/2==0");
	if(A!=0);
   System.out.println(A+"impar");
   else
    System.out.println(B+"par");
	if(B%2==0);
	System.out.println(C+"par");
	
	

	}

 } 
   